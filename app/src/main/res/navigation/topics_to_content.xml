<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/topics_to_navigation"
    app:startDestination="@id/topicsFragment">

    <fragment
        android:id="@+id/navigationBasicFragment"
        android:name="com.lee.androidtopics.navigation.NavigationBasicFragment"
        android:label="fragment_navigation_basic"
        tools:layout="@layout/fragment_navigation_basic" />
    <fragment
        android:id="@+id/topicsFragment"
        android:name="com.lee.androidtopics.TopicsFragment"
        android:label="fragment_topics"
        tools:layout="@layout/fragment_topics">
        <action
            android:id="@+id/action_topicsFragment_to_navigationBasicFragment"
            app:destination="@id/navigationBasicFragment" />
    </fragment>
</navigation>